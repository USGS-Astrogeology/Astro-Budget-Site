{"draw":0,"recordsTotal":{{ tasks|length }},"recordsFiltered":{{ tasks|length }},"data":[
{% for task in proposals[0].tasks %}
  {% if task.staffing is empty %}
["<a href='javascript:void(0)' onclick='editTaskDialog({{ task.taskid }})'>{{ task.taskname }}</a>", "N/A", 0, "$0.00", ""]
  {% endif %}
  {% for staff in task.staffing %}
["<a href='javascript:void(0)' onclick='editTaskDialog({{ task.taskid }})'>{{ task.taskname }}</a>", "{{ staff.name }}", {{ staff.q1hours + staff.q2hours + staff.q3hours + staff.q4hours + staff.flexhours }}, "${{ staff.staffcosts|number_format(2,'.',',') }}", "{{ staff.FY }}"]{% if loop.last !=true %},{% endif %}
  {% endfor %}{% if loop.last !=true %},{% endif %}
{% endfor %}
]}
