{"draw":0,"recordsTotal":{{ conferences|length }},"recordsFiltered":{{ conferences|length }},"data":[
{% for conference in conferences %}
  {% if remote_user[0].admin == 't' %}
["<a href='javascript:void(0)' onclick='editConferenceDialog({{ conference.conferenceid }})'>{{ conference.meeting }}</a>",
  {% else %}
["{{ conference.meeting }}",
  {% endif %}
"{{ conference.conferencerates[0].city }}, {{ conference.conferencerates[0].state }}, {{ conference.conferencerates[0].country }}", "${{ conference.conferencerates[0].perdiem|number_format(2,'.',',') }}", "${{ conference.conferencerate[0].lodging|number_format(2,'.',',') }}", "${{ conference.conferencerates[0].registration|number_format(2,'.',',') }}", "${{ conference.conferencerates[0].groundtransport|number_format(2,'.',',') }}", "${{ conference.conferencerates[0].airfare|number_format(2,'.',',') }}"]{% if loop.last != true %},{% endif %}
{% endfor %}
]}
