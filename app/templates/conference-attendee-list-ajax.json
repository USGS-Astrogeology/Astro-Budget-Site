{"draw":0,"recordsTotal":{{ conferenceattendees|length }},"recordsFiltered":{{ conferenceattendees|length }},"data":[
{% for attendee in conferenceattendees %}
    ["<a href='javascript:void(0)' onclick='editAttendeeDialog({{ attendee.proposal.proposalid }}, {{ attendee.travelid }})'>{{ attendee.meeting }}</a>",
    {{ attendee.travelers }}, 
    "{{ attendee.startdate }}", 
    "{{ attendee.FY }}", 
    {{ attendee.meetingdays }}, 
    {{ attendee.traveldays }}, 
    "{{ attendee.conferencerate[0].airfarecosts|currencyformat }}", 
    "{{ attendee.conferencerate[0].groundtransportcosts|currencyformat }}", 
    "{{ (attendee.conferencerate[0].registrationcosts + attendee.conferencerate[0].othercosts)|currencyformat }}", 
    "{{ attendee.conferencerate[0].perdiemcosts|currencyformat }}", 
    "{{ attendee.conferencerate[0].lodgingcosts|currencyformat }}", 
    "{{ attendee.conferencerate[0].totalcost|currencyformat }}",
    "<a href='javascript:void(0)' onclick='deleteAttendeeDialog({{ attendee.travelid}}, {{ proposals[0].proposalid }})'><img src='images/trash-2-32.png' title='Delete Trip'></a>"]
    {% if loop.last !=true %},{% endif %}{% endfor %}
    ]}
