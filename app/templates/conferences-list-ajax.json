
{"draw":0,"recordsTotal":{{ conferences|length }},"recordsFiltered":{{ conferences|length }},"data":[
  {% for conference in conferences %}
      {% if g.user.admin %}
          ["<a href='javascript:void(0)' onclick='editDialog(\"/conferences/ajax/edit/{{ conference.conferenceid }}\", $(\"#conferencesTable\"))'>{{ conference.meeting }}</a>",
      {% else %}
          ["{{ conference.meeting }}",
      {% endif %}

      {% if conference.conferencerates %}
       {% set conferencerate = conference.conferencerates|geteffective %}
       "{{ conferencerate.city }}, {{ conferencerate.state }}, {{ conferencerate.country }}", 
       "{{ conferencerate.perdiem|currencyformat }}", 
       "{{ conferencerate.lodging|currencyformat }}", 
       "{{ conferencerate.registration|currencyformat }}", 
       "{{ conferencerate.groundtransport|currencyformat }}", 
       "{{ conferencerate.airfare|currencyformat }}",       
      {% else %}
       "", "", "", "", "", "",
      {% endif %}
      "<a href='javascript:void(0)' onclick='deleteDialog(\"/conferences/ajax/delete/{{ conference.conferenceid }}\", $(\"#conferencesTable\"), \"{{ conference.meeting }}\" )'><img src='{{ url_for("static", filename="images/trash-2-16.png") }}'></a>"]
      {% if loop.last != true %},{% endif %}
  {% endfor %}
]}
