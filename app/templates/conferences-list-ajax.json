
{"draw":0,"recordsTotal":{{ conferences|length }},"recordsFiltered":{{ conferences|length }},"data":[
  {% for conference in conferences %}
      {% if g.user.admin %}
          ["<a href='javascript:void(0)' onclick='editDialog(\"/conferences/ajax/edit/{{ conference.conferenceid }}\", null, $(\"#conferencesTable\"))'>{{ conference.meeting }}</a>",
      {% else %}
          ["{{ conference.meeting }}",
      {% endif %}

      {% if conference.conferencerates %}
       "{{ conference.conferencerates[0].city }}, {{ conference.conferencerates[0].state }}, {{ conference.conferencerates[0].country }}", 
       "{{ conference.conferencerates[0].perdiem|currencyformat }}", 
       "{{ conference.conferencerates[0].lodging|currencyformat }}", 
       "{{ conference.conferencerates[0].registration|currencyformat }}", 
       "{{ conference.conferencerates[0].groundtransport|currencyformat }}", 
       "{{ conference.conferencerates[0].airfare|currencyformat }}"]
      {% else %}
       "", "-", "-", "-", "-", "-"]
      {% endif %}
      {% if loop.last != true %},{% endif %}
  {% endfor %}
]}
