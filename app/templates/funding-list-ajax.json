{"draw":0,"recordsTotal":{{ funding|length }},"recordsFiltered":{{ funding|length }},"data":[
{% for fund in funding %}
  {% if g.user.admin %}
    ["<a href='javascript:void(0)' onclick='editDialog(\"/funding/ajax/edit/{{ fund.fundingid }}\", {{ fund.proposalid }}, $(\"#fundingTable\"))'>{{ fund.fiscalyear|fyformat }}</a>",
  {% else %}
    ["{{ fund.fiscalyear|fyformat }}",
  {% endif %}
  "{{ fund.newfunding|currencyformat }}",
  "{{ fund.carryover|currencyformat }}",
  "<a href='javascript:void(0)' onclick='deleteFundingDialog({{ fund.fundingid }}, {{ fund.proposalid }})'><img src='{{ url_for("static", filename="images/trash-2-32.png") }}' title='Delete funding'></a>"]
  {% if loop.last !=true %},{% endif %}
{% endfor %}
]}
